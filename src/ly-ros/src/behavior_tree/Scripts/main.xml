<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="3">
  <BehaviorTree ID="Tree.xml">
    <Sequence>
      <SetAimTargetFromAim ArmorList="{ArmorList}"
                           EnemyHealth="{EnemyHealth}"
                           AimTarget="{AimTarget}"/>
      <SetPosition MyTeam="{MyTeam}"
                   TimeLeft="{TimeLeft}"
                   SelfHealth="{SelfHealth}"
                   AmmoLeft="{AmmoLeft}"
                   FriendRobots="{FriendRobots}"
                   EnemyRobots="{EnemyRobots}"
                   SelfOutpostHealth="{SelfOutpostHealth}"
                   EnemyOutpostHealth="{EnemyOutpostHealth}"
                   TeamBuff="{TeamBuff}"
                              CommandInterval="{CommandInterval}"
                              LastCommandTime="{LastCommandTime}"
                              DestinationID="{DestinationID}"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="SetAimTargetFromAim"
            editable="true">
      <input_port name="ArmorList"
                  default="{ArmorList}"/>
      <input_port name="EnemyHealth"
                  default="{EnemyHealth}"/>
      <output_port name="AimTarget"
                   default="{AimTarget}"/>
    </Action>
    <Action ID="SetNaviPosition"
            editable="true">
      <input_port name="MyTeam"
                  default="{MyTeam}"/>
      <inout_port name="SelfHealth"
                  default="{SelfHealth}" />
      <input_port name="FriendRobots"
                  default="{FriendRobots}"/>
      <input_port name="EnemyRobots"
                  default="{EnemyRobots}"/>
      <input_port name="SelfOutpostHealth"
                  default="{SelfOutpostHealth}"/>
      <input_port name="EnemyOutpostHealth"
                  default="{EnemyOutpostHealth}"/>
      <input_port name="TeamBuff"
                  default="{TeamBuff}"/>
      <input_port name="TimeLeft"
                  default="{TimeLeft}"/>
      <input_port name="AmmoLeft"
                  default="{AmmoLeft}"/>
      <inout_port name="CommandInterval"
                  default="{CommandInterval}"/>
      <inout_port name="LastCommandTime"
                  default="{LastCommandTime}"/>
      <inout_port name="DestinationID"
                  default="{DestinationID}"/>
    </Action>
  </TreeNodesModel>

</root>